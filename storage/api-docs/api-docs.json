    "paths": {
        "/api/products": {
            "get": {
                "tags": [
                    "products"
                ],
                "summary": "Display a listing of the products",
                "operationId": "getProductsByParams",
                "parameters": [
                    {
                        "name": "page",
                        "in": "query",
                        "description": "The page number",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "example": 1
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "ok",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ProductCollection"
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "tags": [
                    "products"
                ],
                "summary": "create a new product",
                "operationId": "createProduct",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/ProductRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "ok",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ProductSchema"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/UnauthorizedResponse"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Unprocessable Content",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/products/{id}": {
            "get": {
                "tags": [
                    "products"
                ],
                "summary": "show product",
                "operationId": "showProduct",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "The ID of product",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        },
                        "example": "1"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "ok",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ProductSchema"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Not found",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                }
            },
            "put": {
                "tags": [
                    "products"
                ],
                "summary": "update product",
                "operationId": "updateProduct",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "The ID of product",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        },
                        "example": "21"
                    }
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/ProductRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "ok",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ProductSchema"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "422": {
                        "description": "Unprocessable Content",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            },
            "delete": {
                "tags": [
                    "products"
                ],
                "summary": "delete product",
                "operationId": "deleteProduct",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "The ID of product",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        },
                        "example": "21"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "ok",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "404": {
                        "description": "Not found",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/login": {
            "post": {
                "tags": [
                    "auth"
                ],
                "summary": "login",
                "operationId": "login",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "example": "test@mail.com"
                                    },
                                    "password": {
                                        "example": "password"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "ok",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                }
            }
        },
        "/api/register": {
            "post": {
                "tags": [
                    "auth"
                ],
                "summary": "register",
                "operationId": "register",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name",
                                    "email",
                                    "password",
                                    "password_confirmation"
                                ],
                                "properties": {
                                    "name": {
                                        "example": "test"
                                    },
                                    "email": {
                                        "example": "test@mail.com"
                                    },
                                    "password": {
                                        "example": "password"
                                    },
                                    "password_confirmation": {
                                        "example": "password"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "ok",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "422": {
                        "description": "Unprocessable Content",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "BaseCollection": {
                "title": "BaseCollection",
                "description": "BaseCollection",
                "properties": {
                    "links": {
                        "$ref": "#/components/schemas/Meta"
                    },
                    "meta": {
                        "$ref": "#/components/schemas/Meta"
                    }
                },
                "type": "object",
                "xml": {
                    "name": "BaseCollection"
                }
            },
            "Meta": {
                "title": "Meta",
                "description": "Meta",
                "properties": {
                    "current_page": {
                        "description": "current page",
                        "type": "number",
                        "example": 1
                    },
                    "from": {
                        "description": "from",
                        "type": "number",
                        "example": 1
                    },
                    "path": {
                        "description": "path",
                        "type": "string",
                        "example": "http://localhost:8000/api/products"
                    },
                    "per_page": {
                        "description": "items per page",
                        "type": "number",
                        "example": 10
                    },
                    "to": {
                        "description": "to",
                        "type": "number",
                        "example": 10
                    },
                    "total": {
                        "description": "total items",
                        "type": "number",
                        "example": 99
                    }
                },
                "type": "object",
                "xml": {
                    "name": "Meta"
                }
            },
            "Links": {
                "title": "Links",
                "description": "Links",
                "properties": {
                    "first": {
                        "description": "first page",
                        "type": "string",
                        "example": "http://localhost:8000/api/products?page=2"
                    },
                    "last": {
                        "description": "last page",
                        "type": "string",
                        "example": "http://localhost:8000/api/products?page=69"
                    },
                    "prev": {
                        "description": "previous page",
                        "type": "string",
                        "example": "http://localhost:8000/api/products?page=1"
                    },
                    "next": {
                        "description": "next page",
                        "type": "string",
                        "example": "http://localhost:8000/api/products?page=3"
                    }
                },
                "type": "object",
                "xml": {
                    "name": "Links"
                }
            },
            "ProductCollection": {
                "title": "ProductCollection",
                "description": "ProductCollection",
                "xml": {
                    "name": "ProductCollection"
                },
                "allOf": [
                    {
                        "$ref": "#/components/schemas/BaseCollection"
                    },
                    {
                        "properties": {
                            "data": {
                                "description": "array of products",
                                "type": "array",
                                "items": {
                                    "$ref": "#/components/schemas/ProductSchema"
                                }
                            }
                        }
                    }
                ]
            },
            "ProductRequest": {
                "title": "ProductRequest",
                "description": "ProductRequest",
                "required": [
                    "name",
                    "sn",
                    "price",
                    "stock"
                ],
                "properties": {
                    "sn": {
                        "$ref": "#/components/schemas/ProductSchema/properties/sn"
                    },
                    "name": {
                        "$ref": "#/components/schemas/ProductSchema/properties/name"
                    },
                    "desc": {
                        "$ref": "#/components/schemas/ProductSchema/properties/desc"
                    },
                    "price": {
                        "$ref": "#/components/schemas/ProductSchema/properties/price"
                    },
                    "stock": {
                        "$ref": "#/components/schemas/ProductSchema/properties/stock"
                    }
                },
                "type": "object",
                "xml": {
                    "name": "ProductRequest"
                }
            },
            "UnauthorizedResponse": {
                "title": "Unauthorized",
                "description": "Unauthorized response",
                "properties": {
                    "message": {
                        "description": "Error message"
                    }
                },
                "type": "object",
                "xml": {
                    "name": "Unauthorized"
                }
            },
            "ProductSchema": {
                "title": "ProductSchema",
                "description": "ProductSchema",
                "properties": {
                    "id": {
                        "description": "Unique ID",
                        "type": "integer",
                        "example": "1"
                    },
                    "sn": {
                        "description": "unique product serial number",
                        "type": "string",
                        "example": "KDX0000001"
                    },
                    "name": {
                        "description": "Product name",
                        "type": "string",
                        "example": "Iphong 669"
                    },
                    "desc": {
                        "description": "Product description",
                        "type": "string",
                        "example": "Iphong 669 description"
                    },
                    "price": {
                        "description": "Product price",
                        "type": "number",
                        "format": "currency",
                        "example": 25000
                    },
                    "stock": {
                        "description": "Product stock",
                        "type": "number",
                        "example": 20
                    },
                    "created_at": {
                        "description": "Product stock",
                        "type": "string",
                        "format": "date",
                        "example": 20
                    },
                    "updated_at": {
                        "description": "Product stock",
                        "type": "string",
                        "format": "date",
                        "example": 20
                    }
                },
                "type": "object",
                "xml": {
                    "name": "ProductSchema"
                }
            }
        },
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "scheme": "bearer"
            }
        }
    }
}